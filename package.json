    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "body-parser": "~1.15.2",
    "chalk": "^1.1.3",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "express": "~4.14.0",
    "foreman": "^2.0.0",
    "mongoose": "^4.7.1",
    "morgan": "~1.7.0",
    "pug": "~2.0.0-beta6",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "~2.3.0",
    "stylus": "0.54.5",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23",
    "gulp": "^3.9.1",
    "del": "^2.2.2",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^1.9.1",
