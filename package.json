{
  "name": "inspireo",
  "version": "0.0.0",
  "license": "MIT",
  "description": "",
  "main": "./bin/www",
  "scripts": {
    "start": "npm run gulp && concurrently --kill-others \"nodemon ./bin/www\" \"npm run ng:serve\" \"npm run gulp:watch\"",
    "debug": "npm run gulp && concurrently --kill-others \"npm run _server:debug\" \"npm run ng:serve\" \"npm run gulp:watch\"",
    "dist": "npm run gulp && ng build -prod && pm2 start main.js",
    "gulp": "node node_modules/gulp/bin/gulp",
    "gulp:watch": "node node_modules/gulp/bin/gulp watch",
    "ng:serve": "node node_modules/angular-cli/bin/ng",
    "ng:build": "node node_modules/angular-cli/bin/ng build --prod",
    "_server:debug": "DEBUG=express:* & nodemon --debug-brk ./bin/www",
    "_server:dist": "NODE_ENV=production nodemon ./bin/www",
    "postinstall": "npm run gulp && npm run ng:build"
  },
  "private": true,
  "engines": {
    "node": "6.3.1"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
